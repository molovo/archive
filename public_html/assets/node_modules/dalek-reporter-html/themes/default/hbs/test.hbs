<li class="{{#if status}}pass{{else}}fail{{/if}}">
    <strong>
        <span class="test-name">{{name}}</span>&nbsp;<span class="browser-name">({{browser}})</span>
        <b class="counts">(<b class="failed">{{failedAssertions}}</b>,<b class="passed">{{passedAssertions}}</b>,{{runnedExpactations}})</b>
    </strong>
    <a href="details/{{id}}.html">Details</a>
    <ol style="display: none;">
        {{#each assertionInfo}}
        <li class="{{#if success}}pass{{else}}fail{{/if}}">
            <span class="test-message">{{#unless message}} Expected: {{expected}} / Actual: {{value}} {{/unless}}{{message}}</span>
        </li>
        {{/each}}
    </ol>
</li>
